{
    "manifest_version": 3,
    "name": "Dynamic Site Tab Counter",
    "version": "1.0",
    "description": "Updates tab title with the count of open tabs for the same site.",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "action": {
      "default_popup": "index.html"
    }
  }